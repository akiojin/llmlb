{
  "mcpServers": {
    "markitdown": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "markitdown-mcp"
      ]
    },
    "llm-router": {
      "type": "stdio",
      "command": "node",
      "args": ["./mcp-server/dist/index.js"],
      "env": {
        "LLM_ROUTER_URL": "http://localhost:51280",
        "LLM_ROUTER_OPENAPI_PATH": "./docs/openapi.yaml"
      }
    }
  }
}
